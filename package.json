{
  "name": "react-newrelic-agent",
  "version": "1.2.0",
  "description": "A React integration SDK for New Relic monitoring, simplifying setup and customization.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "url": "https://github.com/baliyan9887/newrelic-react-sdk"
  },
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run commitlint",
    "build": "tsup",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "new relic",
    "react",
    "monitoring",
    "browser monitoring",
    "performance monitoring",
    "react sdk",
    "new relic browser"
  ],
  "author": "baliyan9887 - Gautam Kumar",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.7",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "whatwg-fetch": "^3.6.20"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/newrelic": "^9.14.3",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^7.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
