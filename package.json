{
  "name": "react-relic-sdk",
  "version": "1.0.0",
  "description": "relic logger sdk",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint '**/*.ts' --fix",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run commitlint",
    "build": "tsup",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "relic",
    "relic-react"
  ],
  "author": "Gautam",
  "license": "ISC",
  "dependencies": {
    "newrelic": "^11.11.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "undici-types": "^6.6.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@types/newrelic": "^9.14.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^7.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
